msgid "Create an account"
msgstr "Créer un compte"

#: nice-login.php:540
msgid "Re-Type Password"
msgstr "Retaper le mot de passe"

#: nice-login.php:544
msgid "Name"
msgstr "Nom"

#: nice-login.php:547
msgid "First Name"
msgstr "Prénom"

#: nice-login.php:550
